@{ 
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <title>Download</title>
</head>

<body>
    <div>
        @foreach(var document in ViewData.Model)
        {
            @Html.ActionLink((string)document, "DownloadDocument", new { filename = document });
            <br />
        }
    </div>
</body>
</html>